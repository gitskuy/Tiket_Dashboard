ion currentStepOnNext() {\n      var step = this.steps[this.currentStep];\n\n      if (step.onNext) {\n        step.onNext();\n      }\n    },\n    currentStepOnBack: function currentStepOnBack() {\n      var step = this.steps[this.currentStep];\n\n      if (step.onBack) {\n        step.onBack();\n      }\n    },\n    isCurrentStepValid: function isCurrentStepValid() {\n      return this.steps[this.currentStep].isValid();\n    },\n    isStepShown: function isStepShown(step) {\n      return step.slot === this.currentSlot && !this.wizardCompleted;\n    },\n    resetWizard: function resetWizard() {\n      this.currentStep = 0;\n      this.wizardCompleted = false;\n      this.$emit('wizardReset');\n    }\n  }\n};",{"version":3,"sources":["VuesticWizard.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6EA,OAAA,yBAAA,MACA,4CADA;AAEA,OAAA,uBAAA,MAAA,0CAAA;AACA,OAAA,qBAAA,MAAA,wCAAA;AACA,OAAA,uBAAA,MAAA,0CAAA;AACA,OAAA,wBAAA,MAAA,4BAAA;AAEA,eAAA;AACA,QAAA,gBADA;AAEA,SAAA;AACA,WAAA,EADA;AAEA,gBAAA;AACA,YAAA,MADA;AAEA,eAAA;AAFA,KAFA;AAMA,kBAAA;AACA,YAAA,MADA;AAEA,eAAA;AAFA,KANA;AAUA,mBAAA;AAAA,eAAA;AAAA,KAVA;AAWA,YAAA,EAXA;AAYA,YAAA;AAZA,GAFA;AAgBA,MAhBA,kBAgBA;AACA,WAAA;AACA,mBAAA,CADA;AAEA,uBAAA,KAFA;AAGA,+BAAA,iBAHA;AAIA,6BAAA,GAJA;AAIA;AACA,sBAAA,KAAA;AALA,KAAA;AAOA,GAxBA;AAyBA,cAAA;AACA,wDADA;AAEA,oDAFA;AAGA,gDAHA;AAIA;AAJA,GAzBA;AA+BA,cAAA;AACA,wBAAA;AADA,GA/BA;AAkCA,YAAA;AACA,eADA,yBACA;AACA,UAAA,WAAA,KAAA,KAAA,CAAA,KAAA,WAAA,EAAA,IAAA;;AACA,UAAA,KAAA,eAAA,EAAA;AACA,mBAAA,KAAA,uBAAA;AACA;;AACA,aAAA,QAAA;AACA,KAPA;AAQA,eARA,yBAQA;AACA,aAAA,KAAA,WAAA,KAAA,CAAA;AACA;AAVA,GAlCA;AA8CA,SA9CA,qBA8CA;AACA,SAAA,GAAA,CAAA,oBAAA,EAAA,KAAA,YAAA;AACA,GAhDA;AAiDA,WAAA;AACA,gBADA,wBACA,MADA,EACA;AACA,WAAA,cAAA,GAAA,MAAA;AACA,KAHA;AAIA,UAJA,oBAIA;AACA,WAAA,iBAAA;;AACA,UAAA,CAAA,KAAA,UAAA,EAAA,IAAA,KAAA,kBAAA,EAAA,EAAA;AACA,aAAA,WAAA;AACA;AACA,KATA;AAUA,UAVA,oBAUA;AACA,WAAA,iBAAA;;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,EAAA;AACA,aAAA,WAA