urcesContent":["<template>\n  <div class=\"demo-container\">\n    <div class=\"demo-container__item\">\n      anchor position\n      <div class=\"anchor\">\n        bottom right\n        <vuestic-popup\n          anchor=\"bottom right\"\n          v-model=\"anchorBottomRight\"\n        >\n          bottom right\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        center middle\n        <vuestic-popup\n          anchor=\"center middle\"\n          v-model=\"anchorCenterMiddle\"\n        >\n          center middle\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        top left\n        <vuestic-popup\n          anchor=\"top left\"\n          v-model=\"anchorTopLeft\"\n        >\n          top left\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      self position\n      <div class=\"anchor\">\n        bottom right\n        <vuestic-popup\n          self=\"bottom right\"\n          v-model=\"selfBottomRight\"\n        >\n          bottom right\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        center middle\n        <vuestic-popup\n          self=\"center middle\"\n          v-model=\"selfCenterMiddle\"\n        >\n          center middle\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        top left\n        <vuestic-popup\n          self=\"top left\"\n          v-model=\"selfTopLeft\"\n        >\n          top left\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      anchor click\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"anchorClickTrue\"\n          :anchorClick=\"true\"\n        >\n          anchor click true\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        false\n        <vuestic-popup\n          v-model=\"anchorClickFalse\"\n          :anchorClick=\"false\"\n        >\n          anchor click false\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      no focus\n      <div class=\"anchor\">\n        true\n        <input type=\"text\">\n        <vuestic-popup\n          v-model=\"noFocusTrue\"\n          :noFocus=\"true\"\n        >\n          <input type=\"text\" placeholder=\"no focus\">\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        false\n        <input type=\"text\">\n        <vuestic-popup\n          v-model=\"noFocusFalse\"\n          :noFocus=\"false\"\n        >\n          <input type=\"text\" placeholder=\"no focus\">\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      no refocus (test doesn't work)\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"noRefocusTrue\"\n          :noRefocus=\"true\"\n        >\n          <input type=\"text\" placeholder=\"no refocus\">\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        false\n        <vuestic-popup\n          v-model=\"noRefocusFalse\"\n          :noRefocus=\"false\"\n        >\n          <input type=\"text\" placeholder=\"no refocus\">\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      max height\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"maxHeightUnset\"\n        >\n          <p v-for=\"n in 30\" :key=\"n\">{{n}} item</p>\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"maxHeightSet\"\n          maxHeight=\"100px\"\n        >\n          <p v-for=\"n in 30\" :key=\"n\">{{n}} item</p>\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      touch position\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"touchPosition\"\n          touchPosition\n        >\n          disable\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      fit\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"fitUnset\"\n          :fit=\"false\"\n        >\n          <p v-for=\"n in 4\" :key=\"n\">{{n}}</p>\n        </vuestic-popup>\n      </div>\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"fitSet\"\n          :fit=\"true\"\n        >\n          <p v-for=\"n in 4\" :key=\"n\">{{n}}</p>\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      disable\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"disable\"\n          disable\n        >\n          disable\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      offset\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"offset\"\n          :offset=\"[14, -114]\"\n        >\n          offset\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      cover\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"cover\"\n          cover\n        >\n          cover cover cover cover cover\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      persistent\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"persistent\"\n          persistent\n        >\n          persistent\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      keep on screen\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"keepOnScreen\"\n          keepOnScreen\n        >\n          keep on screen\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      Event test {{eventTest}}\n      <button @click=\"$refs.eventTest.show('eventTest text')\">Show</button>\n      <button @click=\"$refs.eventTest.hide('eventTest text')\">Hide</button>\n      <button @click=\"$refs.eventTest.toggle('eventTest text')\">Toggle</button>\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          ref=\"eventTest\"\n          v-model=\"eventTest\"\n          persistent\n          @show=\"event => log('show event', event)\"\n          @hide=\"event => log('hide event', event)\"\n        >\n          keep on screen\n        </vuestic-popup>\n      </div>\n    </div>\n    <div class=\"demo-container__item\">\n      close overlay\n      <div class=\"anchor\">\n        true\n        <vuestic-popup\n          v-model=\"closeOverlay\"\n        >\n          <test-component v-close-overlay>Close</test-component>\n        </vuestic-popup>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport VuesticPopup from './VuesticPopup.vue'\nimport { logMixin } from '../../mixins/logMixin'\nimport { closeOverlay } from '../../directives/closeOverlay'\n\nexport default {\n  mixins: [logMixin],\n  directives: {\n    'close-overlay': closeOverlay,\n  },\n  components: {\n    VuesticPopup,\n    TestComponent: {\n      render (h) {\n        return h('div', 'Test component')\n      },\n    },\n  },\n  data () {\n    return {\n      anchorBottomRight: false,\n      anchorCenterMiddle: false,\n      anchorTopLeft: false,\n      selfBottomRight: false,\n      selfCenterMiddle: false,\n      selfTopLeft: false,\n      anchorClickTrue: false,\n      anchorClickFalse: false,\n      noFocusTrue: false,\n      noFocusFalse: false,\n      noRefocusTrue: false,\n      noRefocusFalse: false,\n      maxHeightSet: false,\n      maxHeightUnset: false,\n      touchPosition: false,\n      fitUnset: false,\n      fitSet: false,\n      disable: false,\n      offset: false,\n      cover: false,\n      persistent: false,\n      keepOnScreen: false,\n      closeOverlay: false,\n      eventTest: false,\n    }\n  },\n}\n</script>\n\n<style scoped>\n.anchor {\n  width: 50px;\n  height: 50px;\n  border: solid 1px red;\n  background-color: #4ae387;\n}\n</style>\n"]}]}